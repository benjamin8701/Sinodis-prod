    <validationRules>
        <fullName>CN_Check_Deliver_Address</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
ISPICKVAL(CN_Sample_Delivery_Method__c, &quot;Express Delivery&quot;),
 ISBLANK(CN_Delivery_Address__c  ) 
)</errorConditionFormula>
        <errorDisplayField>CN_Delivery_Address__c</errorDisplayField>
        <errorMessage>Delivery Address is mandatory if delivery method is express delivery.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Opp_Stage_On_Create</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
NOT(ISPICKVAL(CN_Opportunity_Name__r.StageName,&quot;Proposal/Demo/Sample&quot;)),
NOT(ISBLANK(CN_Opportunity_Name__c)),
ISNEW()
)</errorConditionFormula>
        <errorDisplayField>CN_Sample_Request_Status__c</errorDisplayField>
        <errorMessage>Only the opportunity into stage Proposal/Demo/Sample could create Sample Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Prospect_Opportunity</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
IF(CN_Opportunity_Name__r.Account.RecordType.Name =&quot;Prospect&quot;, TRUE, FALSE)
)</errorConditionFormula>
        <errorMessage>sales CANNOT create wholepack sample request for prospect&apos;s opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Check_Sample_Need_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISCHANGED(CN_Sample_Request_Status__c),
  ISPICKVAL(CN_Sample_Request_Status__c, &quot;Ready for Approval&quot;),
 CN_Sample_Needed_Date__c &lt; TODAY()
)</errorConditionFormula>
        <errorDisplayField>CN_Sample_Needed_Date__c</errorDisplayField>
        <errorMessage>Sample Needed Date should not less than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Canceled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  ISCHANGED(CN_Sample_Request_Status__c ) ,
 NOT(( ISPICKVAL(PRIORVALUE(CN_Sample_Request_Status__c ), &quot;Approved&quot;) ) ),
 ISPICKVAL(CN_Sample_Request_Status__c , &quot;Canceled&quot;) 
)</errorConditionFormula>
        <errorDisplayField>CN_Sample_Request_Status__c</errorDisplayField>
        <errorMessage>It&apos;s not allowed cancel sample request which not approved yet.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_After_Approved_Canceled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
	NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
	OR(
          ISPICKVAL(CN_Sample_Request_Status__c, &quot;Approved&quot;),
          ISPICKVAL(CN_Sample_Request_Status__c, &quot;Canceled&quot;),
          ISPICKVAL(PRIORVALUE(CN_Sample_Request_Status__c)  , &quot;Approved&quot;),
          ISPICKVAL(PRIORVALUE(CN_Sample_Request_Status__c)  , &quot;Canceled&quot;)
        ),
	NOT(AND(
	  ISPICKVAL(PRIORVALUE(CN_Sample_Request_Status__c) , &quot;Approved&quot;),
	  ISPICKVAL(CN_Sample_Request_Status__c, &quot;Canceled&quot;)
	)),
        NOT(ISCHANGED(CN_SP_Records_Count_All__c)),
        NOT(ISCHANGED(CN_SP_Records_Count_Delivery_Status_All__c)),
        NOT(ISCHANGED(CN_SP_Records_Count_Delivery_Status_Done__c)),
        NOT(ISCHANGED(CN_Sample_Request_Cost__c))
)</errorConditionFormula>
        <errorMessage>It&apos;s not allowed to edit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_SR_Check_Ready_SKU</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
BEGINS(RecordType.DeveloperName, &quot;CN&quot;),
ISPICKVAL(CN_Sample_Request_Status__c, &quot;Ready for Approval&quot;),
CN_SP_Records_Count_All__c &lt;= 0
)</errorConditionFormula>
        <errorMessage>Please select at least one SKU before submit for approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_SR_Prevent_Cancel</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c, 
	BEGINS(RecordType.DeveloperName,&quot;CN&quot;),
	ISPICKVAL(CN_Sample_Request_Status__c,&quot;Canceled&quot;),  
	NOT(
		IF(DATEVALUE(CN_Approved_Date__c) = TODAY(), 
			IF(TIMEVALUE(CN_Approved_Date__c) &lt; TIMEVALUE(&quot;19:30:00.000&quot;), TIMEVALUE(NOW())&lt;TIMEVALUE(&quot;19:30:00.000&quot;), 
				false 
				),
				IF(DATEVALUE(CN_Approved_Date__c) = TODAY()-1, AND(TIMEVALUE(NOW()) &gt;= TIMEVALUE(&quot;00:00:00.000&quot;), TIMEVALUE(NOW()) &lt; TIMEVALUE(&quot;19:30:00.000&quot;)), false))
		)
	)</errorConditionFormula>
        <errorMessage>You&apos;re not allowed to cancel it now.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_SR_Prevent_Change_Products_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  OR(RecordType.DeveloperName = &quot;CN_Office_Repack&quot;,RecordType.DeveloperName = &quot;CN_Whole_Pack&quot;),
  OR(
    AND(
      CN_SP_Records_Count_Delivery_Status_All__c == CN_SP_Records_Count_All__c,
      CN_SP_Records_Count_Delivery_Status_All__c &gt; 1,
      NOT(ISCHANGED(CN_SP_Records_Count_Delivery_Status_All__c)),
      NOT(ISCHANGED(CN_SP_Records_Count_All__c))
    ),
    AND(
      PRIORVALUE(CN_SP_Records_Count_Delivery_Status_All__c ) ==  PRIORVALUE(CN_SP_Records_Count_All__c),
      PRIORVALUE(CN_SP_Records_Count_Delivery_Status_All__c ) &gt; 0,
      OR(
        ISCHANGED(CN_SP_Records_Count_Delivery_Status_All__c),
        ISCHANGED(CN_SP_Records_Count_All__c)
      )
    )
  ) 
)</errorConditionFormula>
        <errorMessage>You are not allowed to create or eidt the status of sample products when all sample products&apos;s status are not null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_SR_Prevent_Edit_For_Ready</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
BEGINS(RecordType.DeveloperName, &quot;CN&quot;),
AND(
ISPICKVAL(CN_Sample_Request_Status__c, &quot;Ready for Approval&quot;),
NOT(ISCHANGED(CN_Sample_Request_Status__c))
)
)</errorConditionFormula>
        <errorMessage>You are not allowed to edit when sample request is marked as ready for approval.</errorMessage>
    </validationRules>
