NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
 ISBLANK(CN_Actual_Close_Date__c  ),
 ISPICKVAL( StageName , &apos;Closed Won&apos;) 
)</errorConditionFormula>
