        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
OR(ISPICKVAL(StageName, &apos;Closed Lost&apos;),ISPICKVAL(StageName, &apos;Closed Cancelled&apos;)),
 ISBLANK(TEXT(CN_Loss_Reason__c)) 
)</errorConditionFormula>
        <errorDisplayField>CN_Loss_Reason__c</errorDisplayField>
        <errorMessage>Loss Reason is required when Closed Lost or Closed Cancelled</errorMessage>
