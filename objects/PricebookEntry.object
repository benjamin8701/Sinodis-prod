    <validationRules>
        <fullName>CN_CHECK_PRICE_BOOK_ENTRY_CODE</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT(OR(
    CN_Price_Book_Entry_Code__c = (CN_Price_Book_Code__c +  Product2.CN_SKU_Code__c),
    CN_Price_Book_Entry_Code__c = (&apos;LP&apos; +  Product2.CN_SKU_Code__c)
  ))
)</errorConditionFormula>
        <errorDisplayField>CN_Price_Book_Entry_Code__c</errorDisplayField>
        <errorMessage>Price book entry code is incorrect!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PBE_Channel_Check_Discount</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
NOT(Pricebook2.IsStandard),
  CN_Discount_Price__c = 0 ||  ISBLANK(CN_Discount_Price__c ) ||
   CN_Max_Discount_Price__c = 0 || ISBLANK(CN_Max_Discount_Price__c ) 
)</errorConditionFormula>
        <errorMessage>Please input Discount Price and Max Discount Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_PBE_Standard_Check_UoM</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(OR(
   ISCHANGED(CN_Apex_Update_DateTime__c),
  AND( ISNEW() , NOT( ISNULL(CN_Apex_Update_DateTime__c)))
)),
NOT( ISCHANGED( CN_Apex_Update_DateTime__c ) ),
NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
Pricebook2.IsStandard,
ISPICKVAL(CN_Unit_of_Measure__c  , &quot;&quot;) ||  CN_Pricing_Unit__c = 0 ||  ISBLANK(CN_Pricing_Unit__c )
)</errorConditionFormula>
        <errorMessage>Please input Unit of Measure and Pricing Unit for Standard Price Book.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Edit_List_Price</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  NOT($Permission.CN_Marketing_Update_List_Price),
  ISCHANGED( UnitPrice )
)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>You are not allowed to change the list price</errorMessage>
    </validationRules>
