    <validationRules>
        <fullName>CN_Quote_Check_EffectiveExpirationDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  CN_Effective_Date__c &gt; ExpirationDate
)</errorConditionFormula>
        <errorMessage>Expiration Date should be later than Effective Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Check_Effective_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  CN_Effective_Date__c &lt;  TODAY()
)</errorConditionFormula>
        <errorDisplayField>CN_Effective_Date__c</errorDisplayField>
        <errorMessage>Effective Date should be earlier than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Check_Opp_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  ISNEW(),
  NOT(ISPICKVAL(Opportunity.StageName , &apos;Qualification&apos;))
)</errorConditionFormula>
        <errorDisplayField>OpportunityId</errorDisplayField>
        <errorMessage>You can&apos;t create new quote for the opportunity whose stage is not Qualification.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Check_Prices</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  AND( ISBLANK( CN_Approver_1__c )  )
)</errorConditionFormula>
        <errorMessage>第一个审批人不能为空</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Quote_Prevent_Edit_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  RecordType.DeveloperName = &apos;CN_Quote&apos;,
  OR(
    ISPICKVAL(Status, &quot;Approved&quot;),
    ISPICKVAL(PRIORVALUE(Status), &quot;Approved&quot;)
  ),
  OR(
    ISCHANGED(Status),
    ISCHANGED(Name),
    ISCHANGED(CN_Effective_Date__c),
    ISCHANGED(ExpirationDate)
  )
)</errorConditionFormula>
        <errorMessage>You are not allowed to edit when quote is marked as Approved.</errorMessage>
    </validationRules>
