    <compactLayoutAssignment>User_Layout</compactLayoutAssignment>
    <compactLayouts>
        <fullName>User_Layout</fullName>
        <fields>Name</fields>
        <fields>Title</fields>
        <label>User Layout</label>
    </compactLayouts>
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <enableFeeds>true</enableFeeds>
    <externalSharingModel>Private</externalSharingModel>
    <fields>
        <fullName>CN_Is_Finance__c</fullName>
        <externalId>false</externalId>
        <formula>$Profile.Name = &quot;CN CFO&quot; ||  $Profile.Name = &quot;CN Finance AR&quot; || $Profile.Name = &quot;CN Finance Controlling&quot; || $Profile.Name = &quot;CN Finance MDM&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Finance</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CN_Is_Integration_User__c</fullName>
        <externalId>false</externalId>
        <formula>Profile.Name = &apos;System Integration&apos; || Profile.Name = &apos;System Administrator&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Integration User</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CN_Is_Me__c</fullName>
        <externalId>false</externalId>
        <formula>Id =  $User.Id</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Me</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CN_Is_My_Team_Member__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
	OR(
		$User.Id = ManagerId ,
		$User.Id = Manager.ManagerId ,
		$User.Id = Manager.Manager.ManagerId,
		$User.Id = Manager.Manager.Manager.ManagerId,
		$User.Id = Manager.Manager.Manager.Manager.ManagerId
	),
	IF(
		OR(
		     $User.Title =&apos;Sales Rep&apos;,
			$User.Title=&apos;Manager&apos;,
			$User.Title=&apos;Assistant Regional Manager&apos;,
			$User.Title=&apos;Regional Manager&apos;,
			$User.Title=&apos;Channel Manager&apos;,
			$User.Title=&apos;Commerical Director&apos;,
                        $User.Title=&apos;CEO&apos;), 
		True, 
		False
	), 
	False
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is My Team Member</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CN_Is_Sales__c</fullName>
        <externalId>false</externalId>
        <formula>Profile.Name = &quot;CN Sales&quot; ||  Profile.Name = &quot;CN Sales Lead&quot; || Profile.Name = &quot;CN Sales Manager&quot; || Profile.Name = &quot;CN CEO&quot; || Profile.Name = &quot;CN Sales MDM&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Sales</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CN_Sales_Office__c</fullName>
        <externalId>false</externalId>
        <label>Sales Office</label>
        <length>255</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
