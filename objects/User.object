        <trackFeedHistory>false</trackFeedHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CN_User_Personnel_Number__c</fullName>
        <businessStatus>Active</businessStatus>
        <caseSensitive>false</caseSensitive>
        <externalId>true</externalId>
        <label>Personnel Number</label>
        <length>10</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>ManagerId</fullName>
        <trackFeedHistory>true</trackFeedHistory>
        <type>Hierarchy</type>
    </fields>
    <listViews>
        <fullName>ActiveUsers</fullName>
        <columns>FULL_NAME</columns>
        <columns>CORE.USERS.ALIAS</columns>
        <columns>CORE.USERS.USERNAME</columns>
        <columns>CORE.USERS.LAST_LOGIN</columns>
        <columns>CORE.USER_ROLE.NAME</columns>
