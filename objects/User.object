    <sharingModel>Read</sharingModel>
    <validationRules>
        <fullName>CN_Check_PersonnelNumber_Sales</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
  NOT(IF($Profile.Name = &quot;System Integration&quot;, TRUE, FALSE)),
  OR(
       Profile.Name = &apos;CN Sales&apos;,
       Profile.Name = &apos;CN Sales Lead&apos;,
       Profile.Name = &apos;CN Sales Manager&apos;,
       Profile.Name = &apos;CN Sales MDM&apos;,
       Profile.Name = &apos;CN Finance AR&apos;,
       Profile.Name = &apos;CN Finance MDM&apos;,
       Profile.Name = &apos;CN CFO&apos;
  ),
  ISBLANK(CN_User_Personnel_Number__c ) 
)</errorConditionFormula>
        <errorDisplayField>CN_User_Personnel_Number__c</errorDisplayField>
        <errorMessage>Personnel number cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CN_Prevent_Change_Title</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.Trigger_Switcher_Setting__c.EnableValidationRule__c,
NOT(IF($Profile.Name = &quot;System Administrator&quot;, TRUE, FALSE)),
OR(
Profile.Name = &apos;CN Sales&apos;,
Profile.Name = &apos;CN Sales Lead&apos;,
Profile.Name = &apos;CN Sales Manager&apos;,
Profile.Name = &apos;CN Sales MDM&apos;,
Profile.Name = &apos;CN Finance AR&apos;,
Profile.Name = &apos;CN Finance MDM&apos;,
Profile.Name = &apos;CN CFO&apos;
),
ISCHANGED( Title ) 
)</errorConditionFormula>
        <errorDisplayField>Title</errorDisplayField>
        <errorMessage>Only System Admin could change the title</errorMessage>
    </validationRules>
</CustomObject>
